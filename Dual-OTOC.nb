Cell[BoxData[
 RowBox[{
  RowBox[{"otocType1", "[", 
   RowBox[{
   "Jz_", ",", "hz_", ",", "Jx_", ",", "Lr_", ",", "Lt_", ",", "phi_"}], 
   "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"H1", "=", 
     RowBox[{"N", "[", 
      RowBox[{
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{"pauliOp", "[", 
          RowBox[{"i", ",", "1", ",", "Lr"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "Lr"}], "}"}]}], "]"}], "//", 
       "Represent"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"H2", "=", 
     RowBox[{"N", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"pauliOp", "[", 
            RowBox[{"i", ",", "3", ",", "Lr"}], "]"}], "\[CenterDot]", 
           RowBox[{"pauliOp", "[", 
            RowBox[{
             RowBox[{"i", "+", "1"}], ",", "3", ",", "Lr"}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "Lr"}], "}"}]}], "]"}], ")"}], "//", 
       "Represent"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"H3", "=", 
     RowBox[{"N", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{"pauliOp", "[", 
           RowBox[{"i", ",", "3", ",", "Lr"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "Lr"}], "}"}]}], "]"}], ")"}], "//", 
       "Represent"}], "]"}]}], ";", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"phimat", "=", 
     RowBox[{"N", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"Apply", "[", 
         RowBox[{"\[Sigma]", ",", 
          RowBox[{"Table", "[", 
           RowBox[{"3", ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", "Lr"}], "}"}]}], "]"}]}], "]"}], 
        ")"}], "//", "Represent"}], "]"}]}], ";", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"UF", "=", 
     RowBox[{
      RowBox[{"MatrixExp", "[", 
       RowBox[{"I", " ", "Jx", " ", "H1"}], "]"}], ".", 
      RowBox[{"MatrixExp", "[", 
       RowBox[{
        RowBox[{"I", " ", "Jz", " ", "H2"}], "+", 
        RowBox[{"I", " ", "hz", " ", "H3"}]}], " ", "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"res", "=", 
     RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"a", "=", 
     RowBox[{"N", "[", 
      RowBox[{
       RowBox[{"pauliOp", "[", 
        RowBox[{
         RowBox[{"Round", "[", 
          RowBox[{"Lr", "/", "2"}], "]"}], ",", "3", ",", "Lr"}], "]"}], "//",
        "Represent"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"Mesure", " ", 
      RowBox[{"L", "/", "2"}], " ", 
      SubscriptBox["S", "z"]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "1"}], ",", 
      RowBox[{"i", "<", 
       RowBox[{"Lt", "+", "2"}]}], ",", 
      RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"rr", "=", 
        RowBox[{
         RowBox[{"Tr", "[", 
          RowBox[{"a", ".", 
           RowBox[{"MatrixExp", "[", 
            RowBox[{"I", "*", "phi", "*", "H3"}], "]"}], ".", "a", ".", 
           RowBox[{"MatrixExp", "[", 
            RowBox[{
             RowBox[{"-", "I"}], "*", "phi", "*", "H3"}], "]"}]}], "]"}], "/", 
         SuperscriptBox["2", "Lr"]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"res", "=", 
        RowBox[{"Append", "[", 
         RowBox[{"res", ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"i", "-", "1"}], ",", "rr"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"a", "=", 
        RowBox[{
         RowBox[{"ConjugateTranspose", "[", "UF", "]"}], ".", "a", ".", 
         "UF"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
    "\[IndentingNewLine]", "res"}], ")"}]}]], "Code",
